{
    "name": "pigeon",
    "displayName": "Pigeon",
    "description": "syntax highlighting for DynamicPigeon and StaticPigeon",
    "version": "0.0.1",
    "publisher": "BrianWill",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "go-pigeon",
                "aliases": ["GoPigeon", "go-pigeon"],
                "extensions": [".gopigeon"],
                "configuration": "./gopigeon-language-configuration.json"
            },
            {
                "id": "pigeon",
                "aliases": ["Pigeon", "pigeon"],
                "extensions": [".pigeon"],
                "configuration": "./pigeon-language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "go-pigeon",
                "scopeName": "source.gopigeon",
                "path": "./syntaxes/go-pigeon.tmLanguage.json"
            },
            {
                "language": "pigeon",
                "scopeName": "source.pigeon",
                "path": "./syntaxes/pigeon.tmLanguage.json"
            }
        ],
        "themes": [
            {
                "label": "Pigeon",
                "uiTheme": "vs-dark",
                "path": "./themes/Pigeon-color-theme.json"
            }
        ]
    }
}